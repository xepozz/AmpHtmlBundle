{
    "name": "takeit/amp-html-bundle",
    "type": "symfony-bundle",
    "description": "Provide AMP HTML conversion to your Symfony2/Symfony3 projects.",
    "keywords": [
        "amphtml",
        "amp-html",
        "amp",
        "google",
        "html"
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Rafał Muszyński",
            "email": "rmuszynski1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "lullabot/amp": "1.1.*",
        "symfony/framework-bundle": "^3.4 || ^4.1 || ^5.0",
        "symfony/twig-bundle": "^3.4 || ^4.1 || ^5.0",
        "sensio/framework-extra-bundle": "^5.4",
        "doctrine/persistence": "^1.2"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1",
        "sebastian/comparator": "^3.0"
    },
    "autoload": {
        "exclude-from-classmap": [
            "spec/"
        ],
        "psr-4": {
            "Takeit\\Bundle\\AmpHtmlBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Takeit\\Bundle\\AmpHtmlBundle\\spec\\": "spec/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
